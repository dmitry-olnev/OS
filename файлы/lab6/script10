#!/bin/bash
#Передача функциям массивов в качестве аргументов.
#Неправильная конструкция. 
#Доступ есть только к первому элементу массива.

function myfunc {
	echo "Параметры: $@"
	arr=$1
	echo "Полученный массив: ${arr[*]}"
}
myarray=(1 2 3 4 5)
echo "Исходный массив: ${myarray[*]}"
myfunc $myarray